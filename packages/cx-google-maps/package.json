{
    "name": "cx-google-maps",
    "version": "19.9.3",
    "description": "CxJS wrapper for Google Maps",
    "source": "src/index.js",
    "main": "dist/index.js",
    "module": "dist/index.module.js",
    "umd:main": "src/index.umd.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/codaxy/cx-google-maps"
