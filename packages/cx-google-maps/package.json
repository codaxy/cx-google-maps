        "url": "https://github.com/codaxy/cx-google-maps/issues"
    },
    "homepage": "https://codaxy.github.io/cx-google-maps",
    "dependencies": {
        "google-maps-infobox": "^2.0.0",
        "marker-clusterer-plus": "^2.1.4",
        "markerwithlabel": "^2.0.2"
    },
    "devDependencies": {
        "microbundle": "^0.12.0-next.6",
        "prettier": "^1.10.2",
        "rimraf": "^3.0.0",
        "rollup": "^1.20.3",
        "rollup-plugin-babel": "^4.3.3"
